    "ts-node": "10.9.1",
    "typescript": "~4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "webpack-bundle-analyzer": "^4.6.1"
  },
  "overrides": {
    "@nx-plus/docusaurus": {
      "@nrwl/workspace": "$@nrwl/workspace"
    }
  }
}
