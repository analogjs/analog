{
  "name": "vite-plugin-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/vite-plugin-angular/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{workspaceRoot}/{options.outputPath}"],
      "options": {
        "project": "packages/vite-plugin-angular/package.json",
        "outputPath": "node_modules/@analogjs/vite-plugin-angular",
        "entryFile": "packages/vite-plugin-angular/src/index.ts",
        "tsConfig": "packages/vite-plugin-angular/tsconfig.lib.json",
        "compiler": "tsc",
        "external": "all",
        "generateExportsField": true,
        "assets": [
          {
            "input": "packages/vite-plugin-angular",
            "output": "/",
            "glob": "*.md"
          },
          {
            "input": "packages/vite-plugin-angular/migrations",
            "output": "/migrations",
            "glob": "*.json"
          }
        ],
        "format": ["esm", "cjs"],
        "updateBuildableProjectDepsInPackageJson": false
      }
    },
    "test": {
      "executor": "@nx/vite:test"
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "tagPrefix": "vite-plugin-angular@"
      }
    }
  },
  "tags": []
}
