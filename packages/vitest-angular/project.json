{
  "name": "vitest-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/vitest-angular/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build-package": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "node_modules/@analogjs/vitest-angular",
        "main": "packages/vitest-angular/src/index.ts",
        "tsConfig": "packages/vitest-angular/tsconfig.lib.json",
        "assets": [
          "packages/vitest-angular/*.md",
          "packages/vitest-angular/builders.json"
        ],
        "updateBuildableProjectDepsInPackageJson": false,
        "additionalEntryPoints": ["packages/vitest-angular/setup-zone.ts"],
        "generateExportsField": true
      }
    },
    "build-setup-zone": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "node_modules/@analogjs/vitest-angular",
        "main": "packages/vitest-angular/src/index.ts",
        "tsConfig": "packages/vitest-angular/tsconfig.setup-zone.json",
        "updateBuildableProjectDepsInPackageJson": false,
        "clean": false
      }
    },
    "build": {
      "command": "nx build-package vitest-angular && nx build-setup-zone vitest-angular"
    }
  }
}
