{
  "name": "@analogjs/storybook-angular",
  "version": "2.0.4-beta.2",
  "description": "Storybook Integration for Angular & Vite",
  "type": "module",
  "keywords": [
    "storybook",
    "angular",
    "vite"
  ],
  "homepage": "https://analogjs.org",
  "bugs": {
    "url": "https://github.com/analogjs/analog/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/analogjs/analog.git",
    "directory": "packages/storybook-angular"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/brandonroberts"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./preset": "./preset.js",
    "./package.json": "./package.json",
    "./testing": {
      "types": "./src/lib/testing.d.ts",
      "default": "./src/lib/testing.js"
    }
  },
  "main": "src/index.js",
  "dependencies": {
    "@storybook/builder-vite": "^10.0.0"
  },
  "peerDependencies": {
    "@storybook/angular": "^10.0.0",
    "@analogjs/vite-plugin-angular": "*",
    "storybook": "^10.0.0",
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "builders": "src/lib/builders.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "ng-update": {
    "packageGroup": [
      "@analogjs/platform",
      "@analogjs/content",
      "@analogjs/router",
      "@analogjs/storybook-angular",
      "@analogjs/vite-plugin-angular",
      "@analogjs/vite-plugin-nitro",
      "@analogjs/vitest-angular"
    ]
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
