{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "page",
  "title": "Create an Analog page",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "The project where the page will be located.",
      "x-prompt": "Which project will this page be located in?",
      "$default": {
        "$source": "projectName"
      },
      "x-dropdown": "projects"
    }
  },
  "required": ["project"]
}
