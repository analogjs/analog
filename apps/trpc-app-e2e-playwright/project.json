{
  "name": "trpc-app-e2e-playwright",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/trpc-app-e2e-playwright/src",
  "projectType": "application",
  "tags": [],
  "implicitDependencies": ["trpc-app"],
  "targets": {
    "vitest": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/trpc-app-e2e-playwright",
        "command": "vitest run"
      }
    },
    "e2e": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "",
        "command": "start-server-and-test 'nx serve-nitro trpc-app' http://localhost:4205/health 'nx run trpc-app-e2e-playwright:vitest'"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}
